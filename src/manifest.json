{
  "manifest_version": 2,
  "name": "GothStartPage",
  "short_name": "start page plugin",
  "description": ".",
  "version": "1.0.0",
  "author": "jacques917",
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "page": "index.html",
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides" : {
    "bookmarks": "index.html"
  }
}
